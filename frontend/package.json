{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Frontend React app for Foreign Student Verification System",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "npm run build && npx serve -s dist -l $PORT",
